{
  "name": "pricepal-api-v2",
  "version": "1.0.0",
  "description": "Version 2 of the Pricepal API.",
  "author": "Team FOLO",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon app.js --exec",
    "dev-babel": "NODE_ENV=development babel-node --stage 0 app.js",
    "test": "NODE_ENV=testing jest --forceExit",
    "build": "NODE_ENV=production babel src -d build",
    "pretty": "prettier --write --print-width 80 --single-quote --trailing-comma es5 --tab-width 4 --no-semi 'src/**/*.js' 'tests/**/*.js'",
    "start-production": "NODE_ENV=production pm2 start ecosystem.json",
    "clean": "npm cache clean --force",
    "start": "NODE_ENV=production node app.js"
  },
  "license": "MIT",
  "homepage": "https://github.com/foloworld/api.pricepal.com.au#readme",
  "dependencies": {
    "amqplib": "^0.5.2",
    "aws-sdk": "^2.188.0",
    "babel-cli": "^6.5.1",
    "babel-polyfill": "^6.5.0",
    "babel-register": "^6.9.0",
    "bookshelf": "^0.13.3",
    "createsend-node": "^0.8.2",
    "crypto-js": "^3.1.9-1",
    "currency-converter": "^0.1.12",
    "currency-formatter": "^1.4.2",
    "currency-symbol-map": "^4.0.3",
    "customerio-node": "^0.2.0",
    "date-fns": "^1.29.0",
    "dotenv": "^4.0.0",
    "es6-with-traits": "0.0.5",
    "faker": "^4.1.0",
    "fast-csv": "^2.4.1",
    "formidable-upload-koa": "^1.0.1",
    "fs-extra": "^4.0.3",
    "install": "^0.10.4",
    "ioredis": "^3.2.2",
    "jest": "^20.0.4",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
    "kcors": "^2.0.0",
    "knex": "^0.19.5",
    "knex-filter": "^0.1.3",
    "koa": "^2.4.1",
    "koa-bodyparser": "^3.0.0",
    "koa-json-error": "^3.1.2",
    "koa-ratelimit": "^4.0.0",
    "koa-router": "^7.3.0",
    "koa-useragent": "^1.0.0",
    "log4js": "^2.5.2",
    "mailgun.js": "^2.0.1",
    "moment": "^2.20.1",
    "mustache": "^2.3.0",
    "mysql": "^2.15.0",
    "mz": "^2.7.0",
    "node-cache": "^4.2.0",
    "npm": "^5.8.0",
    "pm2": "^2.9.3",
    "r2": "^2.0.1",
    "randexp": "^0.4.5",
    "randomstring": "^1.1.5",
    "slack-node": "^0.1.8",
    "traits-decorator": "^1.0.1",
    "universal-cookie-koa": "^2.1.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.12.0",
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.9.0",
    "nodemon": "^1.14.11",
    "prettier": "^1.10.2",
    "supertest": "^1.2.0"
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy",
      "transform-function-bind",
      "transform-es2015-classes"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ]
    ]
  },
  "keywords": [
    "koa",
    "koa2",
    "api",
    "backend",
    "jwt",
    "pm2",
    "mysql",
    "database",
    "folo"
  ],
  "jest": {
    "testEnvironment": "node"
  }
}
